{
	"name": "vscode-autohotkey",
	"displayName": "AutoHotkey",
	"description": "AutoHotkey language support for VS Code",
	"version": "0.2.2",
	"publisher": "slevesque",
	"engines": {
		"vscode": "^1.0.0"
	},
	"activationEvents": [
		"onLanguage:ahk"
	],
	"main": "./out/extension",
	"categories": [
		"Programming Languages",
		"Snippets"
	],
	"license": "SEE LICENSE IN LICENSE.md",
	"icon": "icon.png",
	"bugs": {
		"url": "https://github.com/stef-levesque/vscode-autohotkey/issues"
	},
	"homepage": "https://github.com/stef-levesque/vscode-autohotkey/blob/master/README.md",
	"repository": {
		"type": "git",
		"url": "https://github.com/stef-levesque/vscode-autohotkey.git"
	},
	"galleryBanner": {
		"color": "#5c2d91",
		"theme": "dark"
	},
	"keywords": [
		"vscode",
		"autohotkey",
		"ahk"
	],
	"contributes": {
		"languages": [{
			"id": "ahk",
			"aliases": ["AutoHotkey", "autohotkey", "ahk"],
			"extensions": [".ahk",".ext"],
			"configuration": "./ahk.configuration.json"
		}],
		"grammars": [{
			"language": "ahk",
			"scopeName": "source.ahk",
			"path": "./syntaxes/ahk.tmLanguage"
		}],
		"snippets": [{
			"language": "ahk",
			"path": "./snippets/ahk.json"
		}]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "npm run compile && node ./node_modules/vscode/bin/test",
		"tslint": "tslint -t verbose src/**/*.ts"
	},
	"devDependencies": {
		"vscode": "^1.1.34",
		"@types/node": "^8.10.25",
		"tslint": "^5.15.0",
		"typescript": "^3.4.3"
	}
}
